{  
  "name": "<%= data.project.name.toLowerCase().replace(/\s/g, "-") %>",
  "description": "<%= data.project.description %>",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "homepage": "<PROJECT_URL>",
  "main": "src/app/layout.tsx",
  "author": {
    "name": "<%= data.author.name %>",
    "email": "<%= data.author.email %>"
  },
  "scripts": {
    "build": "next build",
    "cypress": "cypress open",
    "dev": "next dev",
    "eslint": "next lint",
    "format": "prettier --write --ignore-unknown .",
    "lint": "npm run format &<% if (data.stack.frontend.styling === "SASS Modules") { %> npm run stylelint &<% } %> npm run eslint",
    "prebuild": "npm run lint",
<% if (data.stack.frontend.styling === "SASS Modules") { -%>
    "start": "next start",
    "stylelint": "stylelint **/*.scss"
<% } else { -%>
    "start": "next start"
<% } -%>
  },
  "dependencies": {
<% if (data.stack.frontend.stateManagement === "Redux Toolkit") { -%>
    "@reduxjs/toolkit": "^1.9.5",
<% } -%>
    "@svgr/webpack": "^8.0.1",
    "axios": "^1.4.0",
<% if (data.backend === "Firebase") { -%>
    "firebase": "^9.22.1",
<% } -%>
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
<% if (data.stack.frontend.stateManagement === "Redux Toolkit") { -%>
    "react-redux": "^8.0.7",
<% } -%>
    "sharp": "^0.32.1",
<% if (data.stack.frontend.stateManagement === "Zustand") { -%>
    "uuid": "^9.0.0",
    "zustand": "^4.3.8"
<% } else { -%>
    "uuid": "^9.0.0"
<% } -%>
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
<% if (data.stack.frontend.styling === "Styled Components") { -%>
    "@types/styled-components": "^5.1.26",
<% } -%>
    "@types/uuid": "^9.0.1",
<% if (data.stack.frontend.styling === "TailwindCSS") { -%>
    "autoprefixer": "^10.4.14",
<% } -%>
    "cypress": "^12.13.0",
    "encoding": "^0.1.13",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.4",
    "eslint-plugin-cypress": "^2.13.3",
<% if (data.stack.frontend.styling === "TailwindCSS") { -%>
    "postcss": "^8.4.24",
<% } -%>
<% if (data.stack.frontend.styling === "SASS Modules") { -%>
    "postcss-scss": "^4.0.6",
<% } -%>
    "prettier": "^2.8.8",
<% if (data.stack.frontend.styling === "SASS Modules") { -%>
    "sass": "^1.62.1",
    "stylelint": "^15.7.0",
<% } -%>
<% if (data.stack.frontend.styling === "Styled Components") { -%>
    "styled-components": "^6.0.0-rc.3",
<% } -%>
<% if (data.stack.frontend.styling === "TailwindCSS") { -%>
    "tailwindcss": "^3.3.2",
<% } -%>
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= data.author.githubUsername %>/<%= data.project.name.replace(/\s/g,"") %>.git"
  },
  "keywords": [
<% data.project.keywords.forEach((keyword, index) => { -%>
<% const isLastElement = index === data.project.keywords.length - 1; -%>
<% if (isLastElement) { -%>
    "<%= keyword %>"
<% } else { -%>
    "<%= keyword %>",
<% } -%>
<% }); -%>
  ]
}
