import { NextResponse } from "next/server";

type Data = {
  title: string;
  message: string;
  info: string;
};

export const GET = (): NextResponse<Data> => {
  return NextResponse.json({
    title: "Hello, <%= data.author.name %>!",
    message:
      "You chose the awesome stack: React + NextJS + <%= data.stack.frontend.styling %> + <%= data.stack.frontend.stateManagement %> + <%= data.backend %>",
    info: "Have a nice coding!",
  });
};
