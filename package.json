{
  "name": "sdpm",
  "version": "0.0.4",
  "description": "sdpm CLI",
  "private": false,
  "types": "build/types/types.d.ts",
  "bin": {
    "sdpm": "bin/sdpm"
  },
  "scripts": {
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles --up 1 src/templates/**/*.* ./build",
    "build": "npm run clean-build && npm run compile && npm run copy-templates",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "latest"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
